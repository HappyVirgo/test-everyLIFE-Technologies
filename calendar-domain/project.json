{
  "name": "calendar-domain",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "calendar-domain/src",
  "projectType": "library",
  "tags": [],
  "// targets": "to see all targets run: nx show project calendar-domain --web",
  "targets": {
    "schema-recreate": {
      "executor": "nx:run-commands",
