{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "allowJs": true,
    "outDir": "../dist/out-tsc",
    "module": "commonjs",
    "types": ["cypress", "node", "@4tw/cypress-drag-drop"],
    "sourceMap": false
  },
  "include": [
    "**/*.ts",
    "**/*.js",
    "cypress.config.ts",
    "**/*.cy.ts",
    "**/*.cy.tsx",
    "**/*.cy.js",
    "**/*.cy.jsx",
    "**/*.d.ts"
  ]
}
